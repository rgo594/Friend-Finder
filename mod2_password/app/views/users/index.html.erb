<p id="notice"><%= notice %></p>

<style>
.users {
  background-color: black;
  color: white;
  ;
  padding: 10px;
  font-size: 40px;
}
.coo {
  line-height: 5px;
}
.text {
font-size: 20px;
}
</style>

<div class="users"
  <h1> Users </h1>
</div>

<div class="coo">
  <br></br>
</div>

    <% @users.each do |user| %>

      <img src="<%= user.profile_pic %>">
      <div class="text">
        <strong></strong>
        <%= user.full_name %>
      </div>

      <div class="text">
        <strong>Age:</strong>
        <%= user.age %>
      </div>

      <div class="text">
        <strong>Bio:</strong>
        <%= user.description %>
      </div>




      <div class="text">
      <%= link_to "#{user.first_name}'s profile", user %>
      <br></br>
      </div>

    <% end %>
