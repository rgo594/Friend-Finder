<h1> Events </h1>
<br></br>
<% if current_user %>
  <%= button_to "Create Event", "/events/new", method: :get %>
<% end %>

<br><% @events.each do |event| %>
  <%= link_to event.title, event %>
  <p>location: <%= event.location%>
  date: <%= event.date %></p></br>
<% end %>




<%= link_to 'Back', :back %>
